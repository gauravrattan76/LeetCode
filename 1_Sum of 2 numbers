Two sum -

class Solution {
    public int[] twoSum(int[] nums, int target) {
        int li = nums.length;
        for(int i = 0; i < li -1; i++)
        {
            for(int j = i+ 1; j < li; j++)
            {
            if(target == nums[i] + nums[j])
            {
                return new int[]{i,j};
            }
            }
        }
        return new int[]{};
    }
}



